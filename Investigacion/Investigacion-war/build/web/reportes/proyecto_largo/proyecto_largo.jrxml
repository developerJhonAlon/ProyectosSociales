	<queryString>
		<![CDATA[SELECT SEA_FORMATO_LARGO.SFL_ID,
SEA_FORMATO_LARGO.SFL_BIBLIOGRAFIA,
SEA_CONVOCA.CONVOCA_CODIGO AS NUMERO_CONVOCATORIA,
       SEA_DAT_GENERAL_PROY.DATGEPROY_TITULO AS TITULO_ESPANOL,
       SEA_FORMATO_LARGO.SFL_TITULO_INGLES AS TITULO_INGLES,
       STVSUBJ.STVSUBJ_DESC AS DEPARTAMENTO,
       STVMAJR.STVMAJR_DESC AS CARRERA,
       SEA_GRUPINV.GRUPI_DESCRIP AS GRUPO_INVESTIGACION,
       SEA_PROG_POSTGRADO.POSTPROG_DESCRIP AS PROGRAMA_POSTGRADO,
       SEA_PROGRAMA.PROG_DESCRIP AS NOMBRE_PROGRAMA,
       SUBLINEA.LINEA_DESCIP AS SUBLINEA,
       LINEA.LINEA_DESCIP AS LINEA,
       SEA_PARAMETROS_DET.PRD_DESCRIPCION AS TIPO_INVESTIGACION,
       SEA_FORMATO_LARGO.SFL_PRESUP_RIESGO AS PRESUPUESTO_RIESGO,
       SEA_FORMATO_LARGO.SFL_PRESUP_ESPE AS PRESUPUESTO_ESPE,
       SEA_FORMATO_LARGO.SFL_PRESUP_ENTIDAD_AUSPICIA AS ENTIDAD_AUSPICIANTE,
       SEA_FORMATO_LARGO.SFL_PRESUP_TOTAL AS PRESUPUESTO_TOTAL,
       SEA_DAT_GENERAL_PROY.DATGEPROY_CONSECUENCIAS AS CONSECUENCIAS,
       SEA_DAT_GENERAL_PROY.DATGEPROY_CONSIDERACIONES AS CONSIDERACIONES,
       CATEGORIZACION.PRD_DESCRIPCION AS CATEGORIZACION,
       SEA_FORMATO_LARGO.SFL_NOMBRE_FINAN AS FINANCIACION,
DATGEPROY_PAGILEG_NOMBRE,
SEA_DAT_GENERAL_PROY.DATGEPROY_ID
  FROM SEA_CONVOCA,
       SEA_DAT_GENERAL_PROY,
       SEA_FORMATO_LARGO,
       SEA_DEPCENPROY,
       SATURN.STVSUBJ,
       SATURN.STVMAJR,
       SEA_GRUPINV,
       SEA_PROG_POSTGRADO,
       SEA_PROGRAMA,
       SEA_LINEAINVES SUBLINEA,
       SEA_LINEAINVES LINEA,
       SEA_PARAMETROS_DET,
       SEA_PARAMETROS_DET CATEGORIZACION
 WHERE (SEA_DAT_GENERAL_PROY.CONVOCA_ID = SEA_CONVOCA.CONVOCA_ID)
       AND (SEA_FORMATO_LARGO.DATGEPROY_ID =
               SEA_DAT_GENERAL_PROY.DATGEPROY_ID)
       AND (SEA_DEPCENPROY.DATGEPROY_ID = SEA_DAT_GENERAL_PROY.DATGEPROY_ID)
       AND (SEA_DEPCENPROY.STVSUBJ_CODE_DEPTO = STVSUBJ.STVSUBJ_CODE)
       AND (SEA_DEPCENPROY.STVMAJR_CODE_CAR = STVMAJR.STVMAJR_CODE)
       AND (SEA_DEPCENPROY.GRUPI_ID = SEA_GRUPINV.GRUPI_ID)
       AND (SEA_DEPCENPROY.POSTPROG_ID = SEA_PROG_POSTGRADO.POSTPROG_ID)
       AND (SEA_DAT_GENERAL_PROY.PROG_ID = SEA_PROGRAMA.PROG_ID)
       AND (SEA_DEPCENPROY.LINEA_ID = SUBLINEA.LINEA_ID)
       AND (LINEA.LINEA_ID = SUBLINEA.LINEA_ID_PADRE)
       AND (SEA_DAT_GENERAL_PROY.PRD_ID = SEA_PARAMETROS_DET.PRD_ID)
       AND(SEA_FORMATO_LARGO.PRD_ID_CATEGORIA = CATEGORIZACION.PRD_ID)
       AND (SEA_FORMATO_LARGO.SFL_ID = $P{SFL_ID})]]>
	</queryString>
