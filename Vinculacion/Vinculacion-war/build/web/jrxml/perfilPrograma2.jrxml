	<queryString>
		<![CDATA[select PROG.SPRV_NOMBRE, PROG.SPRV_NOMBRE, PROG.SPRV_COBERTURA, PROG.SPRV_FECHA_INICIO, PROG.SPRV_FECHA_FIN,
(TRUNC(MONTHS_BETWEEN(PROG.SPRV_FECHA_FIN, PROG.SPRV_FECHA_INICIO)/12)) DURACION_ANIOS,
(TRUNC((MONTHS_BETWEEN(PROG.SPRV_FECHA_FIN, PROG.SPRV_FECHA_INICIO)) - (TRUNC(MONTHS_BETWEEN(PROG.SPRV_FECHA_FIN, PROG.SPRV_FECHA_INICIO)/12)*12))) DURACION_MESES,
(TRUNC(PROG.SPRV_FECHA_FIN-(add_months(PROG.SPRV_FECHA_INICIO,trunc(months_between(PROG.SPRV_FECHA_FIN,PROG.SPRV_FECHA_INICIO)/12)*12+trunc(mod(months_between(PROG.SPRV_FECHA_FIN,PROG.SPRV_FECHA_INICIO),12)))))) DURACION_DIAS,
SPRV_METAS, SPRV_POLITICAS, SPRV_VIAVILIDAD_TECNICA, SPRV_PERDERIA_NO_EJECUTA, SPRV_RESULTADOS_IMPACTO,
replace(replace(replace(replace(replace(replace(replace(replace(SPRV_RESUMEN2, 'arial', 'Arial'), 'comic sans ms', 'Comic Sans MS'), 'courier new', 'Courier New'), 'georgia','Georgia'), 'lucida sans unicode', 'Lucida Sans Unicode'), 'tahoma','Tahoma'), 'times new roman', 'Times New Roman'), 'verdana', 'Verdana') SPRV_RESUMEN2,
replace(replace(replace(replace(replace(replace(replace(replace(SPRV_FACTOR_C_EXITO2, 'arial', 'Arial'), 'comic sans ms', 'Comic Sans MS'), 'courier new', 'Courier New'), 'georgia','Georgia'), 'lucida sans unicode', 'Lucida Sans Unicode'), 'tahoma','Tahoma'), 'times new roman', 'Times New Roman'), 'verdana', 'Verdana') SPRV_FACTOR_C_EXITO2,
replace(replace(replace(replace(replace(replace(replace(replace(SPRV_UBICACION2, 'arial', 'Arial'), 'comic sans ms', 'Comic Sans MS'), 'courier new', 'Courier New'), 'georgia','Georgia'), 'lucida sans unicode', 'Lucida Sans Unicode'), 'tahoma','Tahoma'), 'times new roman', 'Times New Roman'), 'verdana', 'Verdana') SPRV_UBICACION2,
replace(replace(replace(replace(replace(replace(replace(replace(SPRV_DIAGNOSTICO2, 'arial', 'Arial'), 'comic sans ms', 'Comic Sans MS'), 'courier new', 'Courier New'), 'georgia','Georgia'), 'lucida sans unicode', 'Lucida Sans Unicode'), 'tahoma','Tahoma'), 'times new roman', 'Times New Roman'), 'verdana', 'Verdana') SPRV_DIAGNOSTICO2,
replace(replace(replace(replace(replace(replace(replace(replace(SPRV_LINEA_BASE2, 'arial', 'Arial'), 'comic sans ms', 'Comic Sans MS'), 'courier new', 'Courier New'), 'georgia','Georgia'), 'lucida sans unicode', 'Lucida Sans Unicode'), 'tahoma','Tahoma'), 'times new roman', 'Times New Roman'), 'verdana', 'Verdana') SPRV_LINEA_BASE2,
replace(replace(replace(replace(replace(replace(replace(replace(SPRV_BENEFICIOS2, 'arial', 'Arial'), 'comic sans ms', 'Comic Sans MS'), 'courier new', 'Courier New'), 'georgia','Georgia'), 'lucida sans unicode', 'Lucida Sans Unicode'), 'tahoma','Tahoma'), 'times new roman', 'Times New Roman'), 'verdana', 'Verdana') SPRV_BENEFICIOS2,
(select  usu.usu_nombres || ' ' || usu.usu_apellidos  from SZDTAVRESPONSABLE_PROG dir inner join  seg_usuario usu on dir.usu_id_docente = usu.usu_id where dir.sztvtipres_code = 1 and DIR.SPRV_ID = PROG.SPRV_ID) DIR_NOMBRE,
(select  usu.usu_cc  from SZDTAVRESPONSABLE_PROG dir inner join  seg_usuario usu on dir.usu_id_docente = usu.usu_id where dir.sztvtipres_code = 1 and DIR.SPRV_ID = PROG.SPRV_ID) DIR_CEDULA,
(select  USU_EMAIL  from SZDTAVRESPONSABLE_PROG dir inner join  seg_usuario usu on dir.usu_id_docente = usu.usu_id where dir.sztvtipres_code = 1 and DIR.SPRV_ID = PROG.SPRV_ID) DIR_MAIL,
(select  USU_TELEFONO_FIJO  from SZDTAVRESPONSABLE_PROG dir inner join  seg_usuario usu on dir.usu_id_docente = usu.usu_id where dir.sztvtipres_code = 1 and DIR.SPRV_ID = PROG.SPRV_ID) DIR_TELEFONO,
(SELECT NBRJOBS_DESC
FROM V_DOCENTE_VINCULACION DOC, SZDTAVRESPONSABLE_PROG DIR
WHERE ROWNUM <=1
AND DOC.USU_ID = DIR.USU_ID_DOCENTE
AND dir.sztvtipres_code = 1
and DIR.SPRV_ID = PROG.SPRV_ID) DIR_CARGO,
(SELECT STVSUBJ_DESC
FROM SZDTAVRESPONSABLE_PROG DIR, STVSUBJ DEP
WHERE DIR.STVSUBJ_CODE_DPTO = DEP.STVSUBJ_CODE
AND dir.sztvtipres_code = 1
and DIR.SPRV_ID = PROG.SPRV_ID) DIR_DEPARTAMENTO,
(SELECT VICERRECTOR_CED FROM SZDTAVCONFIGURACION WHERE ROWNUM=1) VICERRECTOR_CED,
(SELECT VICERRECTOR_NOM FROM SZDTAVCONFIGURACION WHERE ROWNUM=1) VICERRECTOR_NOM,
(SELECT VICERRECTOR_APE FROM SZDTAVCONFIGURACION WHERE ROWNUM=1) VICERRECTOR_APE
from SZDTAVPROGRAMA_V PROG
where PROG.SPRV_ID = $P{idPrograma}]]>
	</queryString>
